{
  "name": "atmosphere-ui",
  "version": "0.6.8",
  "description": "UI for jetstream inertia and Vue 3",
  "keywords": [
    "inertia.js",
    "vuejs",
    "vue3"
  ],
  "scripts": {
    "dev": "vite",
    "build": "npm run create-index-files && vue-tsc --noEmit && vite build",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "serve": "vite preview",
    "create-index-files": "node scripts/create-index-files.js",
    "prepublishOnly": "npm run build",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook --output-dir=docs",
    "chromatic": "npx chromatic --project-token=170dcb534484",
    "generate-declarations": "tsc index.ts --declaration --emitDeclarationOnly --esModuleInterop --outDir dist",
    "release": "standard-version"
  },
  "dependencies": {
    "@interactjs/actions": "^1.10.11",
    "@interactjs/auto-start": "^1.10.11",
    "@interactjs/dev-tools": "^1.10.11",
    "@interactjs/interact": "^1.10.11",
    "@interactjs/modifiers": "^1.10.11",
    "@popperjs/core": "^2.9.2",
    "vueuse-temporals": "^1.0.1"
  },
  "peerDependencies": {
    "@headlessui/vue": "^1.3.0",
    "@inertiajs/inertia": "^0.9.2",
    "@inertiajs/inertia-vue3": "^0.4.7",
    "@vueuse/core": "^5.1.3",
    "tailwindcss": "^3.0.0",
    "vue": "^3.2.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.14.7",
    "@headlessui/vue": "^1.3.0",
    "@inertiajs/inertia": "^0.9.2",
    "@inertiajs/inertia-vue3": "^0.4.7",
    "@rollup/plugin-typescript": "^8.3.0",
    "@storybook/addon-actions": "^6.4.22",
    "@storybook/addon-essentials": "^6.4.22",
    "@storybook/addon-links": "^6.4.22",
    "@storybook/builder-vite": "^0.1.32",
    "@storybook/testing-library": "0.0.11",
    "@storybook/vue3": "^6.4.22",
    "@testing-library/jest-dom": "^5.16.3",
    "@testing-library/user-event": "^13.1.9",
    "@testing-library/vue": "^6.4.2",
    "@types/jest": "^27.0.0",
    "@vitejs/plugin-vue": "^1.2.3",
    "@vitest/ui": "^0.8.1",
    "@vue/compiler-sfc": "^3.2.31",
    "@vue/test-utils": "^2.0.0-0",
    "@vue/vue3-jest": "^27.0.0-alpha.4",
    "@vueuse/core": "^7.5.5",
    "autoprefixer": "^10.4.0",
    "babel-jest": "^27.0.0",
    "chromatic": "^5.9.2",
    "date-fns": "^2.22.1",
    "happy-dom": "^2.52.0",
    "interactjs": "^1.10.11",
    "jest": "^27.0.0",
    "mathjs": "^10.1.1",
    "naive-ui": "^2.16.2",
    "postcss": "^8.4.4",
    "rrule": "^2.6.8",
    "sass": "^1.35.1",
    "standard-version": "^9.3.2",
    "tailwindcss": "^3.0.1",
    "ts-jest": "^27.0.0",
    "typescript": "^4.3.5",
    "vite": "^2.3.8",
    "vitest": "^0.8.1",
    "vue": "^3.2.31",
    "vue-jest": "^5.0.0-0",
    "vue-loader": "^16.5.0",
    "vue-tsc": "^0.3.0"
  },
  "main": "index.js",
  "module": "dist/atmosphere-ui.es.js",
  "files": [
    "scripts/*",
    "src/*",
    "index.js",
    "dist/*"
  ],
  "author": "Jesus Guerrero",
  "license": "MIT",
  "directories": {
    "doc": "docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jesusantguerrero/atmosphere-ui.git"
  },
  "sideEffects": false,
  "bugs": {
    "url": "https://github.com/jesusantguerrero/atmosphere-ui/issues"
  },
  "homepage": "https://github.com/jesusantguerrero/atmosphere-ui#readme",
  "exports": {
    ".": {
      "require": "./dist/atmosphere-ui.umd.js",
      "import": "./dist/atmosphere-ui.es.js"
    },
    "./*": "./dist/*"
  }
}
