{
  "name": "atmosphere-ui",
  "version": "1.1.12",
  "description": "UI for jetstream inertia and Vue 3",
  "type": "module",
  "keywords": [
    "inertia.js",
    "vuejs",
    "vue3"
  ],
  "scripts": {
    "dev": "npm run storybook",
    "build": "run-p type-check build-only",
    "preview": "vite preview --port 4173",
    "create-index-files": "node scripts/create-index-files.js",
    "test:unit": "vitest --environment jsdom",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "start-server-and-test preview http://127.0.0.1:4173/ 'cypress open --e2e'",
    "test:e2e:ci": "start-server-and-test preview http://127.0.0.1:4173/ 'cypress run --e2e'",
    "build-only": "npm run create-index-files && vite build",
    "type-check": "vue-tsc",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "prepublishOnly": "npm run build",
    "storybook": "storybook dev -p 6006",
    "release": "npm run build && bumpp --commit --tag --push",
    "release-publish-only": "bumpp --commit --tag --push",
    "docs:build": "storybook build -o docs",
    "chromatic": "npx chromatic --project-token=170dcb534484"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.7",
    "mathjs": "^11.8.0",
    "vueuse-temporals": "^1.4.3"
  },
  "peerDependencies": {
    "@inertiajs/vue3": "^1.0.0",
    "@vueuse/core": "^9.0.0",
    "tailwindcss": "^3.0.0",
    "vue": "^3.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@inertiajs/vue3": "^1.0.2",
    "@rushstack/eslint-patch": "^1.2.0",
    "@storybook/addon-actions": "^7.0.2",
    "@storybook/addon-essentials": "^7.0.2",
    "@storybook/addon-interactions": "^7.0.2",
    "@storybook/addon-links": "^7.0.2",
    "@storybook/addon-mdx-gfm": "^7.0.2",
    "@storybook/testing-library": "0.1.0",
    "@storybook/vue3": "^7.0.2",
    "@storybook/vue3-vite": "^7.0.2",
    "@testing-library/jest-dom": "^5.16.3",
    "@testing-library/user-event": "^13.1.9",
    "@testing-library/vue": "^7.0.0",
    "@types/jsdom": "^16.2.15",
    "@types/node": "^16.11.41",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vitest/coverage-c8": "^0.25.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.3.2",
    "@vue/tsconfig": "^0.1.3",
    "@vueuse/core": "^9.13.0",
    "autoprefixer": "^10.4.14",
    "axios": "^1.3.5",
    "babel-loader": "^8.3.0",
    "bumpp": "^9.1.0",
    "c8": "^7.13.0",
    "chromatic": "^6.17.3",
    "cypress": "^10.1.0",
    "date-fns": "^2.29.3",
    "eslint": "^8.37.0",
    "eslint-plugin-cypress": "^2.13.2",
    "eslint-plugin-storybook": "^0.6.11",
    "eslint-plugin-vue": "^9.10.0",
    "jsdom": "^20.0.0",
    "naive-ui": "^2.34.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "prettier": "^2.8.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.61.0",
    "standard-version": "^9.5.0",
    "start-server-and-test": "^1.14.0",
    "storybook": "^7.0.2",
    "tailwindcss": "^3.3.1",
    "typescript": "~4.7.4",
    "vite": "^4.2.1",
    "vitest": "^0.26.3",
    "vue": "^3.2.47",
    "vue-loader": "^17.0.1",
    "vue-tsc": "^1.3.12"
  },
  "main": "src/index.js",
  "module": "dist/atmosphere-ui.js",
  "files": [
    "scripts/*",
    "src/*",
    "index.js",
    "dist/*"
  ],
  "author": "Jesus Guerrero",
  "license": "MIT",
  "directories": {
    "doc": "docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jesusantguerrero/atmosphere-ui.git"
  },
  "sideEffects": false,
  "bugs": {
    "url": "https://github.com/jesusantguerrero/atmosphere-ui/issues"
  },
  "homepage": "https://github.com/jesusantguerrero/atmosphere-ui#readme",
  "exports": {
    ".": {
      "require": "./dist/atmosphere-ui.umd.js",
      "import": "./dist/atmosphere-ui.js"
    },
    "./*": "./dist/*"
  }
}
