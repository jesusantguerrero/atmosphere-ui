{
  "name": "atmosphere-ui",
  "version": "1.0.3-beta",
  "description": "UI for jetstream inertia and Vue 3",
  "type": "module",
  "keywords": [
    "inertia.js",
    "vuejs",
    "vue3"
  ],
  "scripts": {
    "dev": "npm run storybook",
    "build": "run-p type-check build-only",
    "preview": "vite preview --port 4173",
    "create-index-files": "node scripts/create-index-files.js",
    "test:unit": "vitest --environment jsdom",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "start-server-and-test preview http://127.0.0.1:4173/ 'cypress open --e2e'",
    "test:e2e:ci": "start-server-and-test preview http://127.0.0.1:4173/ 'cypress run --e2e'",
    "build-only": "npm run create-index-files && vite build",
    "type-check": "vue-tsc",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "prepublishOnly": "npm run build",
    "storybook": "start-storybook -p 6006",
    "docs:build": "build-storybook -o docs",
    "chromatic": "npx chromatic --project-token=170dcb534484"
  },
  "dependencies": {
    "@popperjs/core": "^2.9.2",
    "mathjs": "^10.6.4",
    "vueuse-temporals": "^1.4.2"
  },
  "peerDependencies": {
    "@inertiajs/inertia": "^0.11.0",
    "@inertiajs/inertia-vue3": "^0.6.0",
    "@vueuse/core": "^9.1.3",
    "tailwindcss": "^3.0.0",
    "vue": "^3.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@inertiajs/inertia": "^0.11.0",
    "@inertiajs/inertia-vue3": "^0.6.0",
    "@rushstack/eslint-patch": "^1.1.0",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-interactions": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/builder-vite": "^0.1.39",
    "@storybook/testing-library": "0.0.13",
    "@storybook/vue3": "^6.5.9",
    "@testing-library/jest-dom": "^5.16.3",
    "@testing-library/user-event": "^13.1.9",
    "@testing-library/vue": "^6.4.2",
    "@types/jsdom": "^16.2.14",
    "@types/node": "^16.11.41",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "@vitest/coverage-c8": "^0.25.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.0.0",
    "@vue/tsconfig": "^0.1.3",
    "@vueuse/core": "^7.5.5",
    "autoprefixer": "^10.4.0",
    "axios": "^0.27.2",
    "babel-loader": "^8.2.5",
    "c8": "^7.11.3",
    "chromatic": "^5.9.2",
    "cypress": "^10.1.0",
    "date-fns": "^2.22.1",
    "eslint": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-storybook": "^0.6.4",
    "eslint-plugin-vue": "^9.0.0",
    "interactjs": "^1.10.11",
    "jsdom": "^20.0.0",
    "naive-ui": "^2.16.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.4",
    "prettier": "^2.5.1",
    "rrule": "^2.6.8",
    "sass": "^1.35.1",
    "standard-version": "^9.3.2",
    "start-server-and-test": "^1.14.0",
    "tailwindcss": "^3.1.8",
    "typescript": "~4.7.4",
    "vite": "^3.1.3",
    "vitest": "^0.23.04",
    "vue": "^3.2.40",
    "vue-loader": "^16.8.3",
    "vue-tsc": "^0.40.13"
  },
  "main": "src/index.js",
  "module": "dist/atmosphere-ui.js",
  "files": [
    "scripts/*",
    "src/*",
    "index.js",
    "dist/*"
  ],
  "author": "Jesus Guerrero",
  "license": "MIT",
  "directories": {
    "doc": "docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jesusantguerrero/atmosphere-ui.git"
  },
  "sideEffects": false,
  "bugs": {
    "url": "https://github.com/jesusantguerrero/atmosphere-ui/issues"
  },
  "homepage": "https://github.com/jesusantguerrero/atmosphere-ui#readme",
  "exports": {
    ".": {
      "require": "./dist/atmosphere-ui.umd.js",
      "import": "./dist/atmosphere-ui.js"
    },
    "./*": "./dist/*"
  }
}
